{
  "name": "dbtedman/wordpress-actions-audit",
  "description": "A WordPress plugin that makes a permanent record of actions that occur on the platform for the purpose of auditing them at a later stage.",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "WordPressActionsAudit\\": "php/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WordPressActionsAudit\\": "php/"
    }
  },
  "require": {
    "php": "7.3.*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "8.4.3",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "3.5.2"
  },
  "scripts": {
    "lint": "@php vendor/bin/phpcs -p",
    "format": "@php vendor/bin/phpcbf",
    "test": "@php vendor/bin/phpunit",
    "test:coverage": "@php vendor/bin/phpunit --coverage-clover=coverage.xml"
  }
}
